lay = styled.div`\n  min-height: 50vh;\n  img {\n    width: 100%;\n    height: 100vh;\n    object-fit: cover;\n  }\n`;\n\n// Award Component\nconst Award = ({ title, description }) => {\n  return (\n    <AwardStyle>\n      <h3>{title}</h3>\n      <div className=\"line\"></div>\n      <p>{description}</p>\n    </AwardStyle>\n  );\n};\n\nexport default MovieDetail;\n","/Users/nicolasvarone/Desktop/devbyed/capture/src/movieState.js",[],"/Users/nicolasvarone/Desktop/devbyed/capture/src/animation.js",[],"/Users/nicolasvarone/Desktop/devbyed/capture/src/components/Wave.js",[],"/Users/nicolasvarone/Desktop/devbyed/capture/src/components/Toggle.js",[],{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","severity":1,"message":"80","line":16,"column":18,"nodeType":"81","messageId":"82","endLine":16,"endColumn":27},"no-native-reassign",["83"],"no-negated-in-lhs",["84"],"no-unused-vars","'setMovies' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]